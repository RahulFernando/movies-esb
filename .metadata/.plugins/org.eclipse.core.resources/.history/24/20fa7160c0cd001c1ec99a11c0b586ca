	<http uri-template="http://localhost:4000/api/v1/movies/update" method="put">
		<suspendOnFailure>
			<progressionFactor>1.0</progressionFactor>
		</suspendOnFailure>
		<markForSuspension>
			<retriesBeforeSuspension>0</retriesBeforeSuspension>
			<retryDelay>0</retryDelay>
		</markForSuspension>
	</http>
</endpoint>